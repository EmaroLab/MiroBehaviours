<launch>

<!-- ROBOT NAME --> 
    <param name="robot_name" value="sim01" />

<!-- MQTT BRIDGE -->
    <include $(find mqtt_ros_bridge)/mqtt_ros_bridge-feature-magnetometer/launch/imu_bridge.launch />

<!-- IMU COMPLEMANTARY FILTER -->
    <node name="imu_complementary_filter" pkg="imu_complemntary_filter" type="complmentray_filter_node" 

    <node name="rad2deg" pkg="gb_control" type="rad2deg.py" />

    <node name="attitude2body_vel" pkg="gb_control" type="attitude2body_vel.py" />

<!-- RQT PLOT ROBOT POSITION FROM ODOMETRY -->
    <node name="rqt_plot_angles" pkg="rqt_plot" type="rqt_plot"
    args="/rpy_deg" />

</launch>
